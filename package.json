{
  "name": "pinku",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "ts-node-dev --respawn --transpile-only --poll ./src/index.ts",
    "start": "npx ts-node ./src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/himmeldev/pinku.git"
  },
  "keywords": [],
  "author": "278342221202194434",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/himmeldev/pinku/issues"
  },
  "homepage": "https://github.com/himmeldev/pinku#readme",
  "husky": {
    "hooks": {
      "pre-commit": "yarn dlx @yarnpkg/doctor && lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.ts": "prettier --write",
    "*.js": "prettier --write"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-angular"
    ],
    "rules": {
      "scope-case": [
        2,
        "always",
        "pascal-case"
      ],
      "type-enum": [
        2,
        "always",
        [
          "chore",
          "build",
          "ci",
          "docs",
          "feat",
          "fix",
          "perf",
          "refactor",
          "revert",
          "style",
          "test"
        ]
      ]
    }
  },
  "dependencies": {
    "@types/glob": "^7.1.4",
    "discord.js": "^13.1.0",
    "glob": "^7.2.0",
    "quick.db": "^7.1.3"
  }
}
